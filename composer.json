{
    "name": "blackakula/wdebug",
    "type": "library",
    "description": "PHP Webdriver Debugger",
    "keywords": ["automation","selenium"],
    "license": "OSL 3.0",
    "authors": [
        {
            "name": "Sergii Akulinin",
            "email": "blackakula@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "chrome/webdriver",
                "version": "2.4",
                "dist": {
                    "url": "https://chromedriver.googlecode.com/files/chromedriver_win32_2.3.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-json": "*",
        "facebook/webdriver": "*"
    },
    "require-dev": {
        "claylo/selenium-server-standalone": ">=2.32.0",
        "chrome/webdriver": ">=2.4"
    },
    "autoload": {
        "psr-0": { "": ["src"] }
    }
}
